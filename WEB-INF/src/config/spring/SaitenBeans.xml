		<property name="tranDescScoreHistoryDAO" ref="tranDescScoreHistoryDAO"></property>
	</bean>
	
	<!-- Register score bean configuration  -->
	<bean id="registerScore" class="com.saiten.action.RegisterScoreAction" scope="request">
